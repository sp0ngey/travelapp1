<h1>Youtube Search Results</h1>

<div class="youtube">
  <table>
    <% @ytresult.data.items.each do |search_result| %>
        <tr>
          <td>
            <%= search_result.snippet.title %><br>
          </td>
          <td>
            <iframe width=260 height=210 src="http://www.youtube.com/embed/<%= search_result.id.videoId%>"></iframe>
          </td>
        </tr>
    <% end %>
  </table>
</div>